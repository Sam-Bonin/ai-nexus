{
  "name": "ai-nexus",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "electron": "tsc -p electron/tsconfig.json && electron .",
    "electron:dev": "tsc -p electron/tsconfig.json && electron .",
    "build:next": "next build",
    "build:electron": "next build && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/ && rm -f .next/standalone/.env.local && tsc -p electron/tsconfig.json && electron-builder",
    "package": "yarn build:electron"
  },
  "dependencies": {
    "lucide-react": "^0.545.0",
    "next": "^14.2.18",
    "openai": "^4.73.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.20",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8",
    "eslint-config-next": "^14.2.18",
    "postcss": "^8",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
